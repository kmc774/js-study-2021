Map paging = mew Map();

paging.put("type", "AA");
paging.put("keyword", "key wd")




----------
StringBuffer sb;
for( key in paging){
	sb.append( "&" + key + "=" + paging.get(key) );
}

Sb =>>>>.    "&type=AA&keyword=Key%20wd"






